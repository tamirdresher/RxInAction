<Window x:Class="ObserveAndSubscribeOn.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Rx Search" Height="350" Width="525">
    <DockPanel>

        <StackPanel DockPanel.Dock="Top" Orientation="Vertical">
            <TextBlock Text="Your Text:" Margin="5,5,0,0"/>
            <TextBox x:Name="TextBox" Margin="0,0,0,0"></TextBox>
        </StackPanel>

        <ListBox x:Name="ThrottledResults"></ListBox>
    </DockPanel>
</Window>
